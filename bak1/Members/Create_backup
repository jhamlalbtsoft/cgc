    <script type="text/javascript">
        function SetFileLink(link, id, type) {
            alert("Image uploaded " + link);
            //$("#Link").val(link);
            document.getElementById(id).value = link;
            if (type == '1') {
                $("#" + id + "img").attr("href", link);
                $("#" + id + "img").html(link);
            }
            else {
                $("#" + id + "img").attr("src", link);
            }

        }


        function LoadGroup(id) {
            $("#load").show();
            //alert("hi");
            $.ajax({
                url: "Members/LoadGroup?id=" + id,
                type: "GET",
                data: "",

                success: function (data) {
                    //alert("hi");

                    //$(".cpy").remove();
                    $("#CreateMem").find("#grp").html(data);
                    $("#load").hide();
                    //$.validator.unobtrusive.parse("form");
                },
                error: function (jqXhr, textStatus, errorThrown) {
                    alert("Error '" + jqXhr.status + "' (textStatus: '" + textStatus + "', errorThrown: '" + errorThrown + "')");
                }
                //                complete: function () {
                //                    $("#ProgressDialog").dialog("close");
                //                }
            });
        }
        function LoadSubGroup(id) {
            $("#load").show();
            //alert("hi");
            $.ajax({
                url: "Members/LoadSubGroup?id=" + id + "&GroupId=" + $("#CreateMem").find("#GroupsId").val(),
                type: "GET",
                data: "",

                success: function (data) {
                    //alert("hi");

                    $("#CreateMem").find("#sgrp").html(data);
                    $("#load").hide();
                    $("#CreateMem").find("#SGId").select2();
                    //$.validator.unobtrusive.parse("form");
                },
                error: function (jqXhr, textStatus, errorThrown) {
                    alert("Error '" + jqXhr.status + "' (textStatus: '" + textStatus + "', errorThrown: '" + errorThrown + "')");
                }
                //                complete: function () {
                //                    $("#ProgressDialog").dialog("close");
                //                }
            });
        }

        $("#CreateMem").find("#GroupsId").change(function (event) {
            LoadSubGroup(0);
            $("#CreateMem").find("#sgrplink").attr("href", "<?PHP ECHO webUrl ?>Members/SubGroup-Create?popup=1&eid=AddSubGroup&fname=LoadSubGroup&GroupsId=" + $("#CreateMem").find("#GroupsId").val());
        });

        function LoadDistrict(id) {
            $("#load").show();
            //alert("hi");
            $.ajax({
                url: "Members/LoadDistrict?id=" + id,
                type: "GET",
                data: "",

                success: function (data) {
                    //alert("hi");

                    //$(".cpy").remove();
                    $("#CreateMem").find("#dist").html(data);
                    LoadCity(0);
                    $("#CreateMem").find("#citylink").attr("href", "<?PHP ECHO webUrl ?>Members/City-Create?popup=1&eid=AddCity&fname=LoadCity&DistrictId=" + $("#CreateMem").find("#DistrictId").val());
                    $("#load").hide();
                    //$.validator.unobtrusive.parse("form");
                },
                error: function (jqXhr, textStatus, errorThrown) {
                    alert("Error '" + jqXhr.status + "' (textStatus: '" + textStatus + "', errorThrown: '" + errorThrown + "')");
                }
                //                complete: function () {
                //                    $("#ProgressDialog").dialog("close");
                //                }
            });
        }

        function LoadCity(id) {
            $("#load").show();
            //alert("hi");
            $.ajax({
                url: "Members/LoadCity?id=" + id + "&DistrictId=" + $("#DistrictId").val(),
                type: "GET",
                data: "",

                success: function (data) {
                    //alert("hi");

                    //$(".cpy").remove();
                    $("#CreateMem").find("#city").html(data);
                    LoadArea(0);
                    $("#CreateMem").find("#arealink").attr("href", "<?PHP ECHO webUrl ?>Members/Create?popup=1&eid=AddArea&fname=LoadArea&CityId=" + $("#CreateMem").find("#CityId").val());
                    $("load").hide();
                    //$.validator.unobtrusive.parse("form");
                },
                error: function (jqXhr, textStatus, errorThrown) {
                    alert("Error '" + jqXhr.status + "' (textStatus: '" + textStatus + "', errorThrown: '" + errorThrown + "')");
                }
                //                complete: function () {
                //                    $("#ProgressDialog").dialog("close");
                //                }
            });
        }

        $("#CreateMem").find("#DistrictId").change(function (event) {
            LoadCity(0);
            $("#CreateMem").find("#citylink").attr("href", "<?PHP ECHO webUrl ?>Members/City-Create?popup=1&eid=AddCity&fname=LoadCity&DistrictId=" + $("#CreateMem").find("#DistrictId").val());
        });


        function LoadArea(id) {
            $("#load").show();
            //alert($("#CityId").val());
            $.ajax({
                url: "Members/LoadArea?id=" + id + "&CityId=" + $("#CreateMem").find("#CityId").val(),
                type: "GET",
                data: "",

                success: function (data) {
                    //alert("hi");

                    //$(".cpy").remove();
                    $("#CreateMem").find("#area").html(data);
                    $("#load").hide();
                    //$.validator.unobtrusive.parse("form");
                },
                error: function (jqXhr, textStatus, errorThrown) {
                    alert("Error '" + jqXhr.status + "' (textStatus: '" + textStatus + "', errorThrown: '" + errorThrown + "')");
                }
                //                complete: function () {
                //                    $("#ProgressDialog").dialog("close");
                //                }
            });
        }


        function citychange() {
            LoadArea(0);
            $("#CreateMem").find("#arealink").attr("href", "<?PHP ECHO webUrl ?>Members/Create?popup=1&eid=AddArea&fname=LoadArea&CityId=" + $("#CreateMem").find("#CityId").val());
        }
</script>